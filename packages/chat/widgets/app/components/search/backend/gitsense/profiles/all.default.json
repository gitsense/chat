{
    "name": "all.default",
    "description": "The default search profile used when no specific profile is selected and the search targets imply an overview across multiple types. Defines outputs for the Overview Format, combining fields from individual target profiles.",
    "version": "1.0.0",
    "keywords": ["overview", "all content", "multi-target"],
    "defaults": {
        "targets": ["chats", "messages", "code-blocks"],
        "scope": "all-chats",
        "filters": {},
        "nullFilters": [],
        "notNullFilters": [],
        "options": {
            "caseSensitive": false,
            "wildcard": "implicit"
        },
        "sortBy": [
            {
                "field": "fts_rank",
                "direction": "asc"
            }
        ],
        "pagination": {
            "limit": 5,
            "offset": 0 
        },
        "outputs": {
            "chats": [
                "chats.id",
                "chats.uuid",
                "chats.name",
                "chats.type",
                "chats.deleted",
                "chats.visibility",
                "chats.owner",
                "chats.parent_id",
                "chats.group_id",
                "chats.prompt_id",
                "chats.main_model",
                "chats.pinned",
                "chats.protected",
                "chats.forked_from_msg_id",
                "chats.meta",
                "chats.is_default_name",
                "chats.created_at",
                "chats.updated_at",
                "chats.modified_at",
                "cte.cte_chat_message_stats.num_messages",
                "cte.cte_chat_message_stats.last_updated_at"
            ],
            "messages": [
                "messages.id",
                "messages.chat_id",
                "messages.type",
                "messages.deleted",
                "messages.visibility",
                "messages.parent_id",
                "messages.model",
                "messages.real_model",
                "messages.role",
                "messages.pinned",
                "messages.meta",
                "messages.blob_id",
                "messages.created_at",
                "messages.updated_at",
                "messages.modified_at",
                "messages.meta.file_path",
                "messages.meta.file_name",
                "messages.meta.file_extension",
                "messages.meta.git_repo",
                "messages.meta.published_date",
                "messages.meta.release_version",
                "messages.meta.title",
                "messages.meta.document_type",
                "messages.meta.language",
                "messages.meta.issue_number",
                "messages.meta.state",
                "messages.meta.author",
                "messages.meta.assignees",
                "messages.meta.labels",
                "chats.name"
            ],
            "code-blocks": [
                "code_blocks.uuid",
                "code_blocks.parent_uuid",
                "code_blocks.component",
                "code_blocks.major",
                "code_blocks.minor",
                "code_blocks.patch",
                "code_blocks.header",
                "code_blocks.created_at",
                "code_blocks.header.language",
                "code_blocks.size",
                "chats.name"
            ]
        }
    },
    "emptySearchOutputs": {
        "chats": [
            "chats.id",
            "chats.uuid",
            "chats.name",
            "chats.type",
            "chats.updated_at",
            "cte.cte_chat_message_stats.num_messages",
            "cte.cte_chat_message_stats.last_updated_at"
        ],
        "messages": [
            "messages.id",
            "messages.chat_id",
            "messages.type",
            "messages.role",
            "messages.created_at",
            "messages.updated_at",
            "chats.name"
        ],
        "code-blocks": [
            "code_blocks.uuid",
            "code_blocks.component",
            "code_blocks.created_at",
            "code_blocks.header.language",
            "code_blocks.size",
            "chats.name"
        ]
    },
    "sortBy": [
        {
            "field": "fts_rank",
            "direction": "asc"
        }
    ],
    "emptySearchSortBy": {
        "chats": [
            {
                "field": "cte.cte_chat_message_stats.last_updated_at",
                "direction": "desc"
            }
        ],
        "messages": [
            {
                "field": "messages.updated_at",
                "direction": "desc"
            }
        ],
        "code-blocks": [
            {
                "field": "messages.updated_at",
                "direction": "desc"
            }
        ]
    },
    "rankingPreferences": {
        "description": "Default ranking preferences for overview results."
    }
}
